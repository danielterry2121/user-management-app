{
  "name": "user-management-app",
  "version": "1.0.9",
  "description": "",
  "author": "Matthew A. Raymer",
  "repository": {
    "type": "git",
    "url": "git@gitlab.anomalistdesign.com:eduonix/mongodb-for-beginners/user-management-app.git"
  },
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "npm run start:dev",
    "start:wo-docker": "npm run set:config:dev && node server",
    "start:mock": "npm run set:config:mock && npm run docker:start",
    "start:mock:wo-docker": "npm run set:config:mock && node server",
    "start:dev": "npm run set:config:dev && npm run docker:start",
    "start:prod": "npm run set:config:prod && npm run build:prod && npm run docker:start",
    "build:prod": "npm run lint & webpack -p --progress --profile --colors",
    "build:doc:api": "node_modules/.bin/apidoc -i server/ -o documentation/",
    "lint": "eslint ./client ./server",
    "lint:fix": "eslint --fix ./client ./server",
    "docker:start": "docker-compose up -d",
    "docker:restart": "npm run docker:stop && npm run docker:start",
    "docker:stop": "docker-compose down",
    "set:config:mock": "cp ./config/config.mock.js ./config/config.js",
    "set:config:dev": "cp ./config/config.dev.js ./config/config.js",
    "set:config:prod": "cp ./config/config.prod.js ./config/config.js"
  },
  "engines": {
    "node": ">=6"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "history": "^4.9.0",
    "connect-history-api-fallback": "^1.6.0",
    "ejs": "^2.6.1",
    "email-templates": "^5.1.0",
    "express": "^4.17.1",
    "formik": "^1.5.7",
    "jsonwebtoken": "^8.5.1",
    "mdbreact": "^4.16.0",
    "mongodb": "^3.2.7",
    "nodemailer": "^6.2.1",
    "nodemon": "^1.19.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.1",
    "rxjs": "^6.5.2",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "apidoc": "^0.17.7",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^3.0.0",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.13.0",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.7.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.34.0",
    "webpack-cli": "^3.3.4",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.1"
  }
}
